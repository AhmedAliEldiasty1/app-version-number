# Environment Variables for App Version Manager
# Copy this file to .env.local and update with your values

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_ENABLE_LOGGING=false

# Security
REACT_APP_ENABLE_RATE_LIMITING=true
REACT_APP_MAX_REQUESTS_PER_MINUTE=20

# Feature Flags
REACT_APP_ENABLE_ENCRYPTION=true
REACT_APP_STRICT_MODE=true

# Note: Never commit .env.local with actual secrets!
